 <%- include("../layout/head") %>
 <%- include("../layout/nav") %>
 <link rel="stylesheet" href="/css/body.css">
<br />
<body>

  <div class="aside">
    <%- include("./list_cate") %>
  </div>
  <div class="content">
    <h3 style="color: red">

        Danh sách sản phẩm
 
    </h3>
    <div class="container">
      <div class="row">
        <div class="col-sm-4">
          <div class="thumbnail">
            <img
              class=""
              src="/upload/<%- ca.image %>"
              alt="Sản phẩm 8"
              width="300px"
            />
            <div class="caption">
              <h4 class=""><%- ca.name %></h4>
              <p
                class=""
                style="
                  white-space: nowrap;
                  overflow: hidden;
                  text-overflow: ellipsis;
                "
              >
                <%- ca.note %>
              </p>

              <div class="">
                <p class="lead"><%- ca.price %> VNĐ</p>
              </div>
              <div class="">
                <a
                  class="btn btn-success"
                  href="/user/detail-product/<%- ca._id %>"
                  >Chi tiết</a
                >
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
  <%- include("../layout/footer") %>
</body>


 
 